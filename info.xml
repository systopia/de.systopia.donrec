<?xml version="1.0"?>
<extension key="de.systopia.donrec" type="module">
  <file>donrec</file>
  <name>Donation Receipts</name>
  <description>This extensions can generate and manage donation receipts for tax deductions.</description>
  <license>AGPLv3</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>info@systopia.de</email>
  </maintainer>
  <releaseDate></releaseDate>
  <version>2.4.0-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.60</ver>
  </compatibility>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.donrec</url>
    <url desc="Documentation">http://wiki.civicrm.org/confluence/display/CRM/DonationReceipts+Extension</url>
    <url desc="Support">https://github.com/systopia/de.systopia.donrec/issues</url>
  </urls>
  <comments>Developed by SYSTOPIA. Thanks to Niko Bochan, Thomas Leichtfu&#xDF; and Bj&#xF6;rn Endres.</comments>
  <civix>
    <namespace>CRM/Donrec</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Donrec_Upgrader</upgrader>
</extension>
